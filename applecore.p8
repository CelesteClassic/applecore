-- update

local function _update()

	frames+=1
	frames%=30

	foreach(objects,function(_ENV)
		move(spd.x,spd.y,0);
		(type.update or time)(_ENV)
	end)

	foreach(objects,function(_ENV)
		if type==player then
	  	_g.cam_spdx,_g.cam_spdy=cam_gain*(4+x-cam_x),cam_gain*(4+y-cam_y)
	  	_g.cam_x+=cam_spdx
	  	_g.cam_y+=cam_spdy	
		end
	end)
end
